public class Q1 {
    public static void multiples(int x){
        int count =1;
        for(int i =0;i<3;i++)
        {
            for(int j=0;j<10;j++)
            {
                int output =x*count;
                System.out.print(" "+output );
                count++;     
            }
            System.out.println("");
        }

    }

     public static void main(String[] args) {
        int n =7;
        multiples(n);
    }
