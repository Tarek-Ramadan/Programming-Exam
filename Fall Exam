
import java.util.*;

class HashTable {
    private  int array[];
    private int capacity;
    private int hash (int key) {
    return key % capacity;
    }
    public HashTable (int c) {
      this.capacity = c;
      array = new int[capacity];
      Arrays.fill(array, -1);
    }
    public void insert (int x) {
      int hashcode = hash(x);
      if(array[hashcode] == -1)
      array[hashcode] = x;  
      
      else return;
    }
    public boolean find(int x) {
      int hashcode = hash(x);
      if (array[hashcode] == x)
      return true;
      else return false;
    }
     public static void main(String[] args) {
      HashTable t = new HashTable(15);
      t.insert(10);
      t.insert(10);
      t.insert(20);
      t.find(10);
     System.out.println(Arrays.toString(t.array));

    }
   }

